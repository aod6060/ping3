{
    "type": "profile",
    "version": 1,
    "programs": {
        "compiler": "clang++",
        "archiver": "ar",
        "linker": "clang++"
    },
    "file-extensions": {
        "source-file-extensions": [
            ".cpp",
            ".cxx"
        ],
        "object-file-extension": ".o",
        "binary-file-extension": "",
        "static-lib-extension": ".a",
        "shared-lib-extension": ".so"
    },
    "flags": {
        "include-dir-flag": "-I",
        "lib-dir-flag": "-L",
        "lib-flag": "-l",
        "compile-flag": "-c",
        "object-flag": "-o",
        "static-lib-flag": "-rcs",
        "shared-lib-flag": "-fPIC",
        "shared-lib-flag-linker": "-shared",
        "shared-import-lib-flag": "-Wl,--out-implib,"
    }
}